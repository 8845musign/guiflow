{
  "name": "guiflow",
  "version": "0.1.0",
  "description": "a text editor",
  "main": "dist/main.js",
  "scripts": {
    "start": "npm run dev:build && npm run serve",
    "serve": "DEBUG=true npx electron .",
    "package": "npx gulp package",
    "lint": "npx eslint .",
    "dev": "npx webpack",
    "dev:build": "npm run clean & npm run dev:build:main & npm run dev:build:renderer & npm run dev:build:preload",
    "dev:watch": "npm run clean & npx webpack --watch",
    "dev:build:main": "npx webpack --config-name main",
    "dev:build:renderer": "npx webpack --config-name renderer",
    "dev:build:preload": "npx webpack --config-name preload",
    "dev:serve": "npm run dev:build:main && npm run dev:build:preload && npx webpack-dev-server --config-name renderer",
    "clean": "npx rimraf ./dist",
    "type-check": "tsc --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/hirokidaichi/guiflow.git"
  },
  "author": "8845musign",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/8845musign/guiflow/issues"
  },
  "homepage": "https://github.com/8845musign/guiflow",
  "dependencies": {
    "@monaco-editor/react": "^4.6.0",
    "@types/node": "^20.11.30",
    "ace-builds": "^1.39.0",
    "copy-webpack-plugin": "^13.0.0",
    "css-loader": "^7.1.2",
    "dialogs": "^2.0.1",
    "electron": "^35.0.0",
    "eventemitter3": "^5.0.1",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.6.3",
    "jquery": "^3.7.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rimraf": "^6.0.1",
    "sprintf": "^0.1.5",
    "sprintf-js": "^1.1.3",
    "through2": "^4.0.2",
    "uiflow": "^1.1.8",
    "webpack": "^5.98.0"
  },
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@babel/preset-env": "^7.24.0",
    "@babel/preset-react": "^7.23.3",
    "@babel/preset-typescript": "^7.24.0",
    "@types/jquery": "^3.5.29",
    "@types/react": "^18.2.69",
    "@types/react-dom": "^18.2.22",
    "@types/sprintf-js": "^1.1.4",
    "@types/through2": "^2.0.41",
    "babel-loader": "^9.1.3",
    "eslint": "^9.21.0",
    "eslint-config-prettier": "^10.0.2",
    "eslint-plugin-prettier": "^5.2.3",
    "prettier": "^3.5.3",
    "style-loader": "^3.3.4",
    "ts-loader": "^9.5.1",
    "typescript": "^5.4.5",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.0"
  }
}
