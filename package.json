{
  "name": "guiflow",
  "version": "0.1.0",
  "description": "a text editor",
  "main": "dist/main.js",
  "scripts": {
    "start": "npm run dev:build && npm run serve",
    "serve": "DEBUG=true npx electron .",
    "package": "npx gulp package",
    "lint": "npx eslint .",
    "dev": "npx webpack",
    "dev:build": "npm run clean & npm run dev:build:main & npm run dev:build:renderer & npm run dev:build:preload",
    "dev:watch": "npm run clean & npx webpack --watch",
    "dev:build:main": "npx webpack --config-name main",
    "dev:build:renderer": "npx webpack --config-name renderer",
    "dev:build:preload": "npx webpack --config-name preload",
    "dev:serve": "npm run dev:build:main && npm run dev:build:preload && npx webpack-dev-server --config-name renderer",
    "clean": "npx rimraf ./dist"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/hirokidaichi/guiflow.git"
  },
  "author": "8845musign",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/8845musign/guiflow/issues"
  },
  "homepage": "https://github.com/8845musign/guiflow",
  "dependencies": {
    "ace-builds": "^1.39.0",
    "copy-webpack-plugin": "^13.0.0",
    "css-loader": "^7.1.2",
    "dialogs": "^2.0.1",
    "electron": "^35.0.0",
    "eventemitter3": "^5.0.1",
    "file-loader": "^6.2.0",
    "flumine": "^0.1.8",
    "html-webpack-plugin": "^5.6.3",
    "jquery": "^3.7.1",
    "pinia": "^3.0.1",
    "rimraf": "^6.0.1",
    "sprintf": "^0.1.5",
    "sprintf-js": "^1.1.3",
    "through2": "^4.0.2",
    "uiflow": "^1.1.8",
    "vue": "^3.5.13",
    "vue-loader": "^17.4.2",
    "vue-style-loader": "^4.1.3",
    "vue-template-compiler": "^2.7.16",
    "vue3-ace-editor": "^2.2.4",
    "webpack": "^5.98.0",
    "webpack-cli": "^6.0.1"
  },
  "devDependencies": {
    "eslint": "^9.21.0",
    "eslint-config-prettier": "^10.0.2",
    "eslint-plugin-prettier": "^5.2.3",
    "prettier": "^3.5.3"
  }
}
